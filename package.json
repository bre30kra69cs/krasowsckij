{
  "name": "krasowsckij",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "eslint": "npx eslint ./src --ext .ts,.tsx",
    "prettier": "npx prettier --check ./src",
    "typescript": "npx tsc --build ./tsconfig.json",
    "format": "npm run eslint && npm run prettier",
    "check": "npm run eslint && npm run prettier && npm run typescript",
    "lint-staged": "npm run typescript && npx lint-staged",
    "md:parse": "node ./packages/mdparser"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "highlight.js": "^10.5.0",
    "marked": "^1.2.7",
    "next": "10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/marked": "^1.2.1",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^5.0.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
